class Solution:
    def countPrimes(self, n: int) -> int:

        prime = [1] * (n+1)

        cnt = 0
        if n <= 2:
            return 0

        for i in range(2, n):
            if prime[i]:
                cnt += 1
                for j in range(i*i, n, i):
                    prime[j] = 0
        
        return cnt

-------------------------------------------------------

class Solution:
    def countPrimes(self, n: int) -> int:
        if n <= 2:
            return 0

        is_prime = [1] * n
        is_prime[0] = is_prime[1] = 0

        for i in range(2, int(n ** 0.5) + 1):
            if is_prime[i]:
                is_prime[i * i:n:i] = [0] * ((n - 1 - i * i) // i + 1)

        return sum(is_prime)
