class Solution:
    def isPalindrome(self, x: int) -> bool:
        s = str(x)
        return s == s[::-1]

-----------------------------------------------------

class Solution:
    def isPalindrome(self, x: int) -> bool:
        rev = 0
        temp = x
        while temp>0 :
            rev = rev*10 + temp%10
            temp//=10

        return x == rev

        
